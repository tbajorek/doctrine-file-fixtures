{
  "name": "tbajorek/doctrine-file-fixtures-bundle",
  "description": "A Symfony Bundle to provide simple file fixtures mechanism",
  "keywords": [
    "doctrine",
    "symfony",
    "fixtures"
  ],
  "type": "symfony-bundle",
  "license": "MIT",
  "version": "1.0",
  "require": {
    "php": "^7.2",
    "symfony/framework-bundle": "^4.0|^5.0",
    "symfony/yaml": "^5.0",
    "symfony/console": "^5.0",
    "doctrine/orm": "^2.7",
    "phpoffice/phpspreadsheet": "^1.14"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  },
  "autoload": {
    "psr-4": {
      "Tbajorek\\DoctrineFileFixturesBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tbajorek\\DoctrineFileFixturesBundle\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
